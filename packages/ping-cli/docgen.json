{
  "$schema": "../../node_modules/@effect/docgen/schema.json",
  "name": "p1-cli",
  "projectHomepage": "https://github.com/ryanbas21/ping-cli",
  "homepage": "https://ryanbas21.github.io/ping-cli",
  "srcDir": "src",
  "outDir": "docs",
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "src/main.ts",
    "src/index.ts",
    "src/PingCommand.ts",
    "src/test/**/*",
    "src/Services/**/*",
    "src/HttpClient/**/*",
    "src/Commands/**/*",
    "src/test-helpers/**/*"
  ],
  "enforceDescriptions": false,
  "enforceExamples": false,
  "enforceVersion": false,
  "enableSearchIndex": false,
  "examplesCompilerOptions": {
    "noEmit": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "downlevelIteration": true
  },
  "noErrorOnMissingExamples": true,
  "parseCompilerOptions": {
    "noEmit": false,
    "strict": false,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "moduleResolution": "nodenext",
    "module": "nodenext",
    "target": "ES2022",
    "lib": ["ES2022", "DOM"]
  },
  "noErrorOnExampleCompilationFailure": true
}
