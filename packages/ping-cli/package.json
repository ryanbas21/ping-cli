{
  "name": "p1-cli",
  "version": "0.2.0",
  "description": "A CLI tool for managing PingOne resources",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "p1-cli": "dist/main.js"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    ".env.example"
  ],
  "scripts": {
    "build": "tsc -p ./tsconfig.prod.json",
    "postbuild": "chmod +x dist/main.js",
    "test": "vitest run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "cli",
    "pingone",
    "effect",
    "typescript"
  ],
  "author": "ryanbas21",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanbas21/ping-cli",
    "directory": "packages/ping-cli"
  },
  "bugs": {
    "url": "https://github.com/ryanbas21/ping-cli/issues"
  },
  "homepage": "https://github.com/ryanbas21/ping-cli#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@effect/cli": "^0.72.1",
    "@effect/platform": "^0.93.0",
    "@effect/platform-node": "^0.100.0",
    "dotenv": "^17.2.3",
    "effect": "^3.19.3",
    "keytar": "^7.9.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@effect/vitest": "^0.27.0",
    "@types/node": "^24.10.0",
    "@vitest/coverage-v8": "^4.0.8",
    "publint": "^0.3.15",
    "typescript": "^5.7.2",
    "vitest": "^4.0.8"
  }
}
