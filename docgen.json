{
  "$schema": "node_modules/@effect/docgen/schema.json",
  "name": "ping-cli",
  "projectHomepage": "https://github.com/ryanbas21/ping-cli",
  "homepage": "https://ryanbas21.github.io/ping-cli",
  "srcDir": "packages/ping-cli/src",
  "outDir": "api_docs",
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "packages/ping-cli/src/main.ts",
    "packages/ping-cli/src/PingCommand.ts",
    "packages/ping-cli/src/Services/**/*",
    "packages/ping-cli/src/HttpClient/**/*"
  ],
  "enforceDescriptions": false,
  "enforceExamples": false,
  "enforceVersion": false,
  "enableSearchIndex": false,
  "examplesCompilerOptions": {
    "noEmit": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true
  },
  "noErrorOnMissingExamples": true,
  "parseCompilerOptions": {
    "noEmit": false,
    "strict": false,
    "skipLibCheck": true,
    "moduleResolution": "NodeNext",
    "target": "ES2022",
    "lib": ["ES2022", "DOM"]
  },
  "noErrorOnExampleCompilationFailure": true
}
